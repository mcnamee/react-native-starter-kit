    "@rematch/persist": "^1.1.6",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "expo": "^36.0.2",
    "expo-font": "~8.0.0",
    "firebase": "^7.7.0",
    "native-base": "^2.13.8",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
