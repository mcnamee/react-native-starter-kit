{
  "name": "StarterKit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-react-native",
    "moduleNameMapper": {
     "\\./datepicker$": "<rootDir>/node_modules/tcomb-form-native/lib/templates/bootstrap/datepicker.ios.js",
      "\\./select$": "<rootDir>/node_modules/tcomb-form-native/lib/templates/bootstrap/select.ios.js"
   },
   "preprocessorIgnorePatterns": [
     "node_modules/(?!react-native|tcomb-form-native|apsl-react-native-button)"
   ],
    "collectCoverage": true,
    "verbose": true
  },
  "dependencies": {
    "react": "15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-native": "^0.35.0",
    "react-native-google-analytics-bridge": "^3.1.0",
    "react-native-navbar": "^1.5.4",
    "react-native-side-menu": "^0.20.1",
    "react-native-tab-view": "0.0.29",
    "react-native-vector-icons": "^2.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "tcomb-form-native": "^0.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^16.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "jest": "^16.0.2",
    "jest-react-native": "^16.0.0",
    "mocha": "^3.1.2",
    "react-test-renderer": "^15.3.2"
  }
}
